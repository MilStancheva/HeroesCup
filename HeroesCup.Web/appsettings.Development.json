{
  "Serilog": {
    "Using": [ "Serilog", "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Error"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "heroescup.web.{Timestamp:yyyy.MM.dd} {Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}